    }
    @if(Version.GLES20)
    case GL_DEPTH_TEST: {
      v[0] = as!T(ctx.FragmentOperations.Depth.Test)
    }
    @if(Version.GLES20)
    case GL_DEPTH_WRITEMASK: {
      v[0] = as!T(ctx.Framebuffer.DepthWritemask)
    }
    @if(Version.GLES31)
    case GL_DISPATCH_INDIRECT_BUFFER_BINDING: {
      v[0] = GetID!(T,Buffer)(ctx.Bound.DispatchIndirectBuffer)

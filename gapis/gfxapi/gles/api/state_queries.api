      write(v[0:1])
    }
    @if(Version.GLES20)
    case GL_DEPTH_BITS: {
      write(v[0:1])
    }
    @if(Version.GLES20)
    case GL_DEPTH_CLEAR_VALUE: {
      v[0] = as!T(ctx.Framebuffer.DepthClearValue)
    }
    @if(Version.GLES20)
    case GL_DEPTH_FUNC: {
      v[0] = as!T(ctx.FragmentOperations.Depth.Func)
    }
    @if(Version.GLES20)
    case GL_DEPTH_RANGE: {
      s := v[0:2]
      s[0] = as!T(ctx.Rasterization.DepthRange[0])
